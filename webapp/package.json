{
  "name": "sb-digital-library",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run pre-build && ng serve -o",
    "start-standalone": "npm run pre-build && ng serve --c standalone -o",
    "start-stage": "npm run pre-build && ng serve --c stage -o",
    "build": "npm run pre-build && ng build --prod --aot --vendor-chunk",
    "build-stage": "npm run pre-build && ng build --c stage --aot --vendor-chunk",
    "build-ci": "npm run pre-build-ci && ng build --c standalone --aot --vendor-chunk",
    "build-standalone": "npm run pre-build && ng build --c standalone --aot --vendor-chunk",
    "build-debug": "npm run pre-build && ng build",
    "build-standalone-debug": "npm run pre-build && ng build --c standalone",
    "pre-build": "node stamp-version && npm run sprites && npm run test",
    "pre-build-ci": "node stamp-version && npm run sprites && npm run test-ci",
    "test": "ng test --source-map=false --watch=false",
    "test-ci": "ng test --source-map=false --watch=false --browsers=ChromeNoSandbox",
    "test-debug-chrome": "ng test --browsers=ChromeDebug",
    "test-debug-firefox": "ng test --browsers=FirefoxDebug",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node stamp-version",
    "sprites": "svg-sprite --config sprite-config.json src/assets/svg/*.svg"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@material/button": "^3.1.0",
    "@material/chips": "^3.2.0",
    "@material/icon-button": "^3.1.0",
    "@material/ripple": "^3.1.0",
    "@material/textfield": "^3.2.0",
    "@okta/okta-angular": "^1.2.2",
    "angulartics2": "^7.5.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "git-describe": "^4.0.4",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.17.0",
    "svg-sprite": "^1.5.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
