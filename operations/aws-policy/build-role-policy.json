{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Resource": [
                "arn:aws:logs:us-west-2:021548413574:log-group:/aws/codebuild/digital-library-webapp",
                "arn:aws:logs:us-west-2:021548413574:log-group:/aws/codebuild/digital-library-webapp:*"
            ],
            "Action": [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ]
        },
        {
            "Effect": "Allow",
            "Resource": [
                "arn:aws:s3:::codepipeline-us-west-2-*"
            ],
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:GetObjectVersion",
                "s3:GetBucketAcl",
                "s3:GetBucketLocation"
            ]
        },
        {
            "Effect": "Allow",
            "Resource": [
                "arn:aws:s3:::logs.webapp.dl.smarterbalanced.com",
                "arn:aws:s3:::build.webapp.dl.smarterbalanced.org",
                "arn:aws:s3:::logs.webapp.dl.smarterbalanced.com/*",
                "arn:aws:s3:::build.webapp.dl.smarterbalanced.org/*"
            ],
            "Action": [
                "s3:PutObject",
                "s3:GetBucketAcl",
                "s3:GetBucketLocation"
            ]
        }
    ]
}
